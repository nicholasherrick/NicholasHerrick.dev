{{> nav-block}}
<div class="container">
    <a id="logout-button" href="/logout"><button type="button">Logout</button></a>
    <div class="row">
        <div class="col-6">
            <form>
                <div class="form-group">
                    <label for="project-name">Project Name:</label>
                    <input type="text" class="form-control" id="project-name">
                </div>
                <div class="form-group">
                    <label for="deployed-link">Demo Link:</label>
                    <input type="text" class="form-control" id="deployed-link">
                </div>
                <div class="form-group">
                    <label for="repository-link">Repository Link:</label>
                    <input type="text" class="form-control" id="repository-link">
                </div>
                <div class="form-group">
                    <label for="project-description">Project Description:</label>
                    <textarea class="form-control" id="project-description" rows="3"></textarea>
                </div>
                <div class="form-group">
                    <label for="image-path">Path to Image:</label>
                    <input type="text" class="form-control" id="image-path">
                    <small class="form-text text-muted">Image must be added to repo first</small>
                </div>
                <button id="submit-project" type="submit">Submit</button>
            </form>
        </div>
    </div>
</div>
{{> footer-block}}
{{> scripts-block}}